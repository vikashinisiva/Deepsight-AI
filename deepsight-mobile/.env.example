# DeepSight AI Environment Configuration

# DeepSeek AI API Configuration
EXPO_PUBLIC_DEEPSEEK_API_URL=https://api.deepseek.com/v1
EXPO_PUBLIC_DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Fallback AI Service (optional)
EXPO_PUBLIC_FALLBACK_API_URL=https://api.example.com/v1
EXPO_PUBLIC_FALLBACK_API_KEY=your_fallback_api_key_here

# Local AI Model Configuration
EXPO_PUBLIC_ENABLE_LOCAL_MODELS=true
EXPO_PUBLIC_LOCAL_MODEL_PATH=./models/

# Analysis Configuration
EXPO_PUBLIC_DEFAULT_CONFIDENCE_THRESHOLD=0.5
EXPO_PUBLIC_MAX_OBJECTS=20
EXPO_PUBLIC_MAX_COLORS=8

# App Configuration
EXPO_PUBLIC_API_TIMEOUT=30000
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true

# Development Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=info
